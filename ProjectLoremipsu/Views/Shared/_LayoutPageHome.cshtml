                                <script text="text/script">
                                    (function ($) {
                                        function ActionForm() {
                                            $('.form-clda').hide();
                                            $('#property_type').change(function () {
                                                if ($(this).val() == 'apartment-for-rent-in-hcmc') {
                                                    $('.form-clda').show();
                                                    $('.form-clda option[value="0"]').text('-Buildings-');
                                                } else {
                                                    $('.form-clda').hide();
                                                    $('#child_location').val("0").change();
                                                }
                                            });
                                            $('#child_location').change(function () {
                                                if ($(this).val() != '0') {
                                                    $('#child_location').attr('name', 'property_location');
                                                } else {
                                                    $('#child_location').attr('name', 'child_location');
                                                }
                                            })
                                        }
                                        $(document).ready(ActionForm);
                                        $(document).ajaxComplete(function () {
                                            ActionForm()
                                        });
                                    })(jQuery);
                                </script>
